#! /usr/bin/env node

var file = require(process.cwd()+'/package.json');
var pkg = JSON.parse(JSON.stringify(file));
var spawn = require('child_process').spawn;

if ( pkg.trello ) {
  spawn('open', [pkg.trello]);
} else {
  console.error("--------------------------------------");
  console.error("Couldnt find the url for "+pkg.name);
  console.error("--------------------------------------");
}
